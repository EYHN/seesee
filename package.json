{
  "name": "seesee",
  "version": "1.0.0",
  "description": "Image Viewer for react.",
  "main": "./lib/index.js",
  "repository": "https://github.com/EYHN/seesee.git",
  "author": "eyhn <cneyhn@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./lib && rimraf ./example",
    "dev": "webpack-dev-server --inline --progress",
    "build": "npm run clean && tsc && webpack --config webpack.config.example.js && webpack --config webpack.config.prod.js && npm run build:noview",
    "build:noview": "cross-env noview=true suffix=\"-noview\" webpack --config webpack.config.prod.js",
    "lint": "tslint --force --format verbose \"src/**/*.{ts,tsx}\""
  },
  "devDependencies": {
    "@types/aphrodite": "^0.5.7",
    "@types/react": "16.0.40",
    "@types/react-dom": "16.0.4",
    "@types/recompose": "0.24.5",
    "awesome-typescript-loader": "3.5.0",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "file-loader": "1.1.11",
    "jest": "22.4.2",
    "tslint": "5.9.1",
    "tslint-react": "3.5.1",
    "typescript": "2.7.2",
    "url-loader": "1.0.1",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.1"
  },
  "dependencies": {
    "aphrodite": "^1.2.5",
    "html-webpack-plugin": "3.0.4",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-overdrive": "^0.0.9",
    "recompose": "0.26.0"
  }
}
